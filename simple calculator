#include <iostream>
using namespace std;

int main() {


	char op ;
	float num1 , num2 ;
	
	cin >> op;
	cin >> num1 >> num2 ;
	
	switch (op) 
	{
		 
		 case '+':
		 cout << num1 + num2 ;
		 break;
		 
		 case '-':
		 cout << num1 - num2 ;
		 break;
		 
		 case '*':
		 cout << num1 * num2 ;
		 break;
		 
		 case '/':
		 cout << num1 / num2 ;
		 break;
		 
		 default :
		 cout << "error"
		 break;
		 
  }

	return 0;
}
/* your input should be the option first like + for addition , - for substraction , * for multiplication , / for divide 
followed by the number1 and the number 2 u want to apply operation to */
